<![CDATA[
if show_all_system then lock "show_system"
values{2} "gs_detlevel_3D_m" igs_detlevel_3D_m, tgs_detlevel_3D_m
values{2} "gs_detlevel_2D_m" igs_detlevel_3D_m, tgs_detlevel_3D_m
]]>
