! coord_vin[inx_vin][1] - X начала, в мм модели! coord_vin[inx_vin][2] - Y начала, в мм модели! coord_vin[inx_vin][3] - X точки сгиба, в мм модели! coord_vin[inx_vin][4] - Y точки сгиба, в мм модели! coord_vin[inx_vin][5] - Длина полки, в мм модели! coord_vin_s[inx_vin][1] - X начала, в мм макета! coord_vin_s[inx_vin][2] - Y начала, в мм макета! coord_vin_s[inx_vin][3] - X точки сгиба, в мм макета! coord_vin_s[inx_vin][4] - Y точки сгиба, в мм макета! coord_vin_s[inx_vin][5] - Длина полки, в мм макетаEPS = 0.0001  if inx_vin=0 then inx_vin=1unID = 900*inx_vinnt=2if vardim2(coord_vin)<2*nt+1 or vardim1(coord_vin)<inx_vin or vardim2(coord_vin_s)<2*nt+1 or vardim1(coord_vin_s)<inx_vin then	dim coord_vin_t[][]	for i=1 to inx_vin		for j=1 to 2*nt+1			coord_vin_t[i][j]=0.1*j		next j	next i	coord_vin=coord_vin_t : coord_vin_s=coord_vin_t	parameters coord_vin=coord_vin, coord_vin_s=coord_vin_sendifAngleT = 0if coord_vin[inx_vin][3]>EPS and coord_vin[inx_vin][4]>EPS then AngleT=ATN(coord_vin[inx_vin][4]/coord_vin[inx_vin][3])if coord_vin[inx_vin][3]>EPS and coord_vin[inx_vin][4]<EPS then AngleT=ATN(coord_vin[inx_vin][4]/coord_vin[inx_vin][3])+360if coord_vin[inx_vin][3]<EPS and abs(coord_vin[inx_vin][3])>EPS then AngleT=ATN(coord_vin[inx_vin][4]/coord_vin[inx_vin][3])+180if abs(coord_vin[inx_vin][3])<EPS and coord_vin[inx_vin][4]>EPS then AngleT=90if abs(coord_vin[inx_vin][3])<EPS and coord_vin[inx_vin][4]<EPS then AngleT=270AngleT=AngleT-90if AngleT+90>90 and AngleT+90<180 and coord_vin[inx_vin][5]>0 then coord_vin[inx_vin][5] = coord_vin[inx_vin][5] * (-1)if AngleT+90>0 and AngleT+90<90 and coord_vin[inx_vin][5]<0 then coord_vin[inx_vin][5] = coord_vin[inx_vin][5] * (-1)if AngleT+90>180 and AngleT+90<270 and coord_vin[inx_vin][5]>0 then coord_vin[inx_vin][5] = coord_vin[inx_vin][5] * (-1)if AngleT+90>270 and AngleT+90<360 and coord_vin[inx_vin][5]<0 then coord_vin[inx_vin][5] = coord_vin[inx_vin][5] * (-1)has_autotext = (STRSTR(fstr_vin, "<")>0 and STRSTR(fstr_vin, ">")>0) or (STRSTR(sstr_vin, "<")>0 and STRSTR(sstr_vin, ">")>0)