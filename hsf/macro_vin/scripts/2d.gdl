if strlen(fstr_vin)<EPS and strlen(sstr_vin)<EPS then endPAPER_TO_MODEL = GLOB_SCALE / 1000DEFINE STYLE "defult" Shrift , Shrift_razm, 5, Shrift_tipSET STYLE "defult"n = REQUEST ("Height_of_style", "defult", height)height = height * PAPER_TO_MODELr = request ("View_Rotangle", "", _angleViewRot)_totalRotate	= (SYMB_ROTANGLE + _angleViewRot) MOD 360ang = _totalRotate * (SYMB_MIRRORED - not(SYMB_MIRRORED))if use_hotspot then	coord_vin[inx_vin][1] = coord_vin_s[inx_vin][1]*PAPER_TO_MODEL	coord_vin[inx_vin][2] = coord_vin_s[inx_vin][2]*PAPER_TO_MODEL	HOTSPOT2 0, coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 1+128+1024, coord_vin_s[inx_vin][1]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 2+1024, coord_vin_s[inx_vin][1]: unID=unID+1	HOTSPOT2 -1, coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 3+1024, coord_vin_s[inx_vin][1]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], 0, unID, coord_vin[inx_vin][2], 1+128+1024, coord_vin_s[inx_vin][2]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], coord_vin[inx_vin][2], unID, coord_vin[inx_vin][2], 2+1024, coord_vin_s[inx_vin][2]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], -1, unID, coord_vin[inx_vin][2], 3+1024, coord_vin_s[inx_vin][2]: unID=unID+1else	HOTSPOT2 0, coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 1+128 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 2 : unID=unID+1	HOTSPOT2 -1, coord_vin[inx_vin][2], unID, coord_vin[inx_vin][1], 3 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], 0, unID, coord_vin[inx_vin][2], 1+128 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], coord_vin[inx_vin][2], unID, coord_vin[inx_vin][2], 2 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][1], -1, unID, coord_vin[inx_vin][2], 3 : unID=unID+1endifadd2 coord_vin[inx_vin][1], coord_vin[inx_vin][2]rot2 angn_transf = 2if SYMB_MIRRORED then 	mul2 -1,1	n_transf = 3endifif use_hotspot then	coord_vin[inx_vin][3] = coord_vin_s[inx_vin][3]*PAPER_TO_MODEL	coord_vin[inx_vin][4] = coord_vin_s[inx_vin][4]*PAPER_TO_MODEL	coord_vin[inx_vin][5] = coord_vin_s[inx_vin][5]*PAPER_TO_MODEL	HOTSPOT2 0, coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 1+128+1024, coord_vin_s[inx_vin][3]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 2+1024, coord_vin_s[inx_vin][3]: unID=unID+1	HOTSPOT2 -1, coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 3+1024, coord_vin_s[inx_vin][3]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], 0, unID, coord_vin[inx_vin][4], 1+128+1024, coord_vin_s[inx_vin][4]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4], unID, coord_vin[inx_vin][4], 2+1024, coord_vin_s[inx_vin][4]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], -1, unID, coord_vin[inx_vin][4], 3+1024, coord_vin_s[inx_vin][4]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 1+128+1024, coord_vin_s[inx_vin][5]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3]+coord_vin[inx_vin][5], coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 2+1024, coord_vin_s[inx_vin][5]: unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3]-1, coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 3+1024, coord_vin_s[inx_vin][5]: unID=unID+1else	HOTSPOT2 0, coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 1+128 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 2 : unID=unID+1	HOTSPOT2 -1, coord_vin[inx_vin][4], unID, coord_vin[inx_vin][3], 3 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], 0, unID, coord_vin[inx_vin][4], 1+128 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4], unID, coord_vin[inx_vin][4], 2 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], -1, unID, coord_vin[inx_vin][4], 3 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3], coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 1+128 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3]+coord_vin[inx_vin][5], coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 2 : unID=unID+1	HOTSPOT2 coord_vin[inx_vin][3]-1, coord_vin[inx_vin][4]+0.005, unID, coord_vin[inx_vin][5], 3 : unID=unID+1endifif not(has_autotext) then	StrMax = max(stw(fstr_vin), stw(sstr_vin)) * PAPER_TO_MODEL	StrMax = max(StrMax, abs(coord_vin[inx_vin][5]))else	StrMax = max(stw(" ") * PAPER_TO_MODEL, abs(coord_vin[inx_vin][5]))endifif abs(coord_vin[inx_vin][5])<EPS then 	if coord_vin[inx_vin][3]<=0 and coord_vin[inx_vin][4]>EPS then StrMax=-StrMax	if coord_vin[inx_vin][3]<=0 and coord_vin[inx_vin][4]<=0 then StrMax=-StrMaxelse	sgnx = sgn(coord_vin[inx_vin][5])	StrMax=StrMax*sgnxendifanchor = 6 : if StrMax<EPS then anchor = 4DEFINE STYLE "fstr" Shrift , Shrift_razm, anchor, Shrift_tipSET STYLE "fstr"text2 coord_vin[inx_vin][3]+StrMax, coord_vin[inx_vin][4]+height*0.4, fstr_vintext2 coord_vin[inx_vin][3]+StrMax, coord_vin[inx_vin][4]-height*0.7, sstr_vinline2 0,0,coord_vin[inx_vin][3], coord_vin[inx_vin][4]line2 coord_vin[inx_vin][3], coord_vin[inx_vin][4], coord_vin[inx_vin][3] + StrMax, coord_vin[inx_vin][4]del n_transf!Добавить перенос строки