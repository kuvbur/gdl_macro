!dict coord!coord.error = "-"!call "macro_get_vert_sheet" parameters coord = coord, RETURNED_PARAMETERS coord!if coord.error = "ok" then !endifdict polythcoord.error = "-"if haskey(coord.out) then _d = REMOVEKEY (coord.out)if not(haskey(coord.in)) then 	coord.error = coord.error + "; not(haskey(coord.in))"	goto "m_end"endif!Нужно ли высчитывать скругление?flag_fold = 0if haskey(coord.fold) then	if coord.fold = 1 then		flag_fold = 1	endifendifsgnth = 1if haskey(coord.sgnth) then 	sgnth = coord.sgnth	if abs(sgnth)<EPS then sgnth = 1endifif flag_fold = 1 then	call "macro_parse" parameters marc_coord = coord, RETURNED_PARAMETERS coordelse	coord.out = coord.in	coord.error = "ok"endifif coord.error = "ok" then	polyth.error = "-"	polyth.in = coord.out	polyth.thickness = coord.thickness*sgnth	call "macro_offset_poly" parameters mop_poly_dict = polyth, RETURNED_PARAMETERS polyth	if polyth.error = "ok" then		coord.out = polyth.out		coord.length = polyth.length		coord.sum_length = polyth.sum_length	endif	coord.error = polyth.errorelse	goto "m_end"endifgoto "m_end""m_end":exit coord