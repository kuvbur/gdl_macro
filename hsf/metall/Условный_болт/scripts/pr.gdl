spec_all = 1success = LIBRARYGLOBAL ("Libraryflobals-km", "ignore_id", ignore_id)spec_bolt_t = 1 : IF STRSTR (GLOB_ID, ignore_id, 1) > 0 THEN spec_bolt_t = 0if spec_all=1 then spec_all = spec_bolt_tcall "Показ_по_сборкам" PARAMETERS ALL subpos=subpos, pos=GLOB_ID RETURNED_PARAMETERS show_subpos, show_markerif spec_all=1 then spec_all = show_subpos*show_markerif spec_all=1 then 	call "Показ_по_типам" PARAMETERS tip_konstr=tip_konstr RETURNED_PARAMETERS show_type	if show_type = 0 then endendifif spec_bolt*spec_all then call "Спецификатор" parameters tip_el=tip_el,								subpos = subpos, 								pos = GLOB_ID, 								obozn=obozn,								naen=naen,								ves=ves,								kol_vo_spec=kol_vo_spec,								komment=kommentif spec_wash*spec_all then 	if (wash2_thk*((qty_wash_start=3) or (qty_wash_start=2)) + wash2_thk*((qty_wash_end=3) or (qty_wash_end=2)))>0 then		ves_p = 7850 * wash2_thk		s_plit_obsh = wash2_a * wash2_b		naen = "--" + str("%.0mm", wash2_thk) + "x" + str("%.0mm", min(wash2_a,wash2_b)) + "x" + str("%.0mm", max(wash2_a,wash2_b))		tip_profile = "-- " + str('%.0mm', wash2_thk)		n_wash2 = ((qty_wash_start=3) or (qty_wash_start=2)) + ((qty_wash_end=3) or (qty_wash_end=2))		for i=1 to n_wash2*kol_vo_spec			call "Спецификатор" parameters tip_el="20",											subpos = subpos, 											pos = GLOB_ID+".1",											tip_konstr="Прочие",											stal_gost="ГОСТ 27772-88",											mater="С245",											obozn="ГОСТ 19903-74",											tip_profile=tip_profile,											dlin_prof=s_plit_obsh,											ves=ves_p, 											naen=naen		next i	endifendif