EPS = 0.0001show_type=0flag_del = 0flag_add = 1filter_type = 0 : success = LIBRARYGLOBAL ("Libraryflobals-km", "filter_type", filter_type)if filter_type<EPS then goto "end_show"space = " " : var = tip_konstr : gosub "trim_space" : tip_konstr = varspace = " " : var = tip_konstr : gosub "trim_space" : tip_konstr = varif tip_konstr = " " or strlen(tip_konstr)=0 then goto "end_show"tip_konstr = STRTOLOWER(tip_konstr)dim arr[]dim hide_type_clear[] : hide_type_clear[1] = ""success = LIBRARYGLOBAL ("Libraryflobals-km", "hide_type_clear", hide_type_clear)if hide_type_clear[1] = "*" or hide_type_clear[1] = " " or strlen(hide_type_clear[1])=0 then 	flag_del = 0else	arr = hide_type_clear : gosub "check_show" : flag_del = flagendifif not(flag_del) then	dim show_type_clear[] : show_type_clear[1] = "*"	success = LIBRARYGLOBAL ("Libraryflobals-km", "show_type_clear", show_type_clear)	if show_type_clear[1] = "*" or show_type_clear[1] = " " or strlen(show_type_clear[1])=0 then 		flag_add = 1	else		arr = show_type_clear : gosub "check_show" : flag_add = flag	endifendif"end_show":show_type=flag_add and not(flag_del)exit show_type"check_show":	flag = 0	for i=1 to vardim1(arr)		if arr[i] = "*" then flag = 1		if not(flag) then			if arr[i] = tip_konstr then 				flag = 1			else				if strstr(arr[i],"*")>0 then					first_expr = (STRSUB(arr[i], 1, 1) = "*") !Начинается с *					end_expr = (STRSUB(arr[i], STRLEN(arr[i]), 1) = "*") !Заканчивается на *					tarr = arr[i]					if first_expr then tarr = STRSUB(tarr, 2, STRLEN(tarr))					if end_expr then tarr = STRSUB(tarr, 1, STRLEN(tarr)-1)					if first_expr and end_expr then						flag = (STRSTR(tip_konstr, tarr)>0)					else						ttip_konstr = "*"						if first_expr then ttip_konstr = STRSUB(tip_konstr, 2, STRLEN(tarr))						if end_expr then ttip_konstr = STRSUB(tip_konstr, 1, STRLEN(tarr))						flag = 	(ttip_konstr = tarr)					endif				endif			endif		endif		if flag then i=vardim1(arr)	next ireturn"trim_space":	if vartype(var) = 2 then		DO			is_trim = 0			lvar = strlen(var)			is_first_space = (strstr(STRSUB (var, 1, 1),space)>0)			is_end_space = (strstr(STRSUB (var, lvar, 1),space)>0)			if is_first_space then				var = STRSUB (var, 2, lvar-1)				lvar = strlen(var)			endif			if is_end_space then var = STRSUB (var, 1, lvar-1)			is_trim = is_first_space + is_end_space		WHILE is_trim <> 0	endifreturn