dim show_by_param[] : show_by_param[1] = ""success = LIBRARYGLOBAL ("Libraryflobals-classfilter", "show_by_param", show_by_param)dim show_by_val[] : show_by_val[1] = ""success = LIBRARYGLOBAL ("Libraryflobals-classfilter", "show_by_val", show_by_val)success = LIBRARYGLOBAL ("Libraryflobals-classfilter", "show_invert", show_invert)show = show_invertDIM folderNamesArray[]n = APPLICATION_QUERY ("OwnCustomParameters", "GetParameterFolderNames()", folderNamesArray)for i = 1 to vardim1(folderNamesArray) step 3		DIM parNamesArray[]		querystring = "GetParameterNames(" + folderNamesArray[i] + ")"		n = APPLICATION_QUERY ("OwnCustomParameters", querystring, parNamesArray)		if folderNamesArray[i] = "Main.Folder" or folderNamesArray[i] = "IFC.Folder" then			for j = 1 to vardim1(parNamesArray) step 3				for k=1 to vardim1(show_by_param)					if parNamesArray[j] = show_by_param[k] or parNamesArray[j] = "Main."+show_by_param[k] or parNamesArray[j] = "IFC."+show_by_param[k] then						querystring = "GetParameter(" + parNamesArray[j] + ")"						n = APPLICATION_QUERY ("OwnCustomParameters", querystring, parValue)						if show_by_val[k] = parValue then 							show = not(show_invert)							goto "m_end"						endif						k = vardim1(show_by_param)					endif				next k			next j		endifnext i"m_end":	end show