dim reinforce_gost[8]dim reinforce_class[8]dim reinforce_diam[16][8]dim reinforce_weight[16][8]success = LIBRARYGLOBAL ("Libraryflobals-kzh", "reinforce_class", reinforce_class)success = LIBRARYGLOBAL ("Libraryflobals-kzh", "reinforce_gost", reinforce_gost)success = LIBRARYGLOBAL ("Libraryflobals-kzh", "reinforce_diam", reinforce_diam)success = LIBRARYGLOBAL ("Libraryflobals-kzh", "reinforce_weight", reinforce_weight)gost = ""nstolb = 0nstrok = 0weight = 0if diam_arm<=0 then diam_arm = 24/1000if class="" then class="A500C"for i = 1 to vardim1(reinforce_class)	if class = reinforce_class[i] then 		nstolb = i		i = vardim1(reinforce_class)	endifnext ifor i = 1 to vardim1(reinforce_diam)	if diam_arm = reinforce_diam[i][nstolb] then 		nstrok = i		i = vardim1(reinforce_diam)	endifnext iweight = reinforce_weight[nstrok][nstolb]gost = reinforce_gost[nstolb]PARAMETERS weight=weight, gost=gostend weight, gost