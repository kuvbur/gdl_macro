!CALL "Сортамент_арматуры" parameters all!values "class" reinforce_classdim reinforce_gost[] : reinforce_gost[1] = ""dim reinforce_class[] : reinforce_class[1] = ""dim reinforce_diam[][] : reinforce_diam[1][1] = 0dim reinforce_weight[][] : reinforce_diam[1][1] = 0FILE_DEPENDENCE "Сортамент_арматуры.txt"ch6=OPEN("TEXT","Сортамент_арматуры.txt","SEPARATOR=';',MODE=RO,LIBRARY")i = 1 : gost_in="" : class_in="" : diam_in=0 : area_in=0 : weight_in=0 : nclass=1 : ndiam=1REPEAT	n=INPUT(ch6, i+1, 1, gost_in, class_in, diam_in, area_in, weight_in)	if reinforce_class[nclass] = "" then		reinforce_class[nclass]= class_in		reinforce_gost[nclass]= gost_in	endif	if class_in <> reinforce_class[nclass] then  		nclass = nclass + 1		reinforce_class[nclass]= class_in		reinforce_gost[nclass]= gost_in		ndiam = 1	endif	if reinforce_diam[ndiam][nclass] = 0 then 		reinforce_diam[ndiam][nclass] = diam_in / 1000		reinforce_weight[ndiam][nclass] = weight_in	endif	if diam_in/1000 <> reinforce_diam[ndiam][nclass] then 		ndiam = ndiam + 1		reinforce_diam[ndiam][nclass] = diam_in / 1000		reinforce_weight[ndiam][nclass] = weight_in	endif	i = i + 1UNTIL n = -1	close (ch6)end reinforce_gost, reinforce_class, reinforce_diam, reinforce_weight!"Class2Gost":!	nstolb = 0!	for i = 1 to vardim1(reinforce_class)!		if class = reinforce_class[i] then nstolb = i!	next i!	gost = reinforce_gost[nstolb]!return!!"GetDiametr":!	i = 1 : usl = 0!	if nstolb = 0 then nstolb = 1!	REPEAT!		diam_temp[i] = reinforce_diam[i][nstolb]!		i = i + 1!		if i <= vardim1(reinforce_diam) then!			usl = (reinforce_diam[i][nstolb] = 0)!		else!			usl = 1!		endif!	UNTIL usl!return!!!"Diametr2Weight":!	nstrok = 1!	if nstolb = 0 then nstolb = 1!	for i = 1 to vardim1(reinforce_diam)!		if diametr = reinforce_diam[i][nstolb] then nstrok = i!	next i!	weight = reinforce_weight[nstrok][nstolb]!return