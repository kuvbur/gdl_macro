! Разбивает строку mss_in_str по разделителю mss_delim_char! Использование:!dim mss_out_array[] : mss_delim_char = ";" : mss_in_str = "string1;string2"!call "macro_split_string" parameters mss_in_str=mss_in_str,!							mss_delim_char=mss_delim_char,!							RETURNED_PARAMETERS mss_out_arrayEPS = 0.0001dim _out_array[] : _out_array[1] = ""if strlen(mss_in_str)<EPS then goto "m_end"if strlen(mss_delim_char)<EPS then goto "m_end"if STRSTR(mss_in_str, mss_delim_char)<EPS then	_out_array[1] = mss_in_str	goto "m_end"endif_i = 1_currStr = mss_in_strwhile STRSTR(_currStr, mss_delim_char)>EPS and _i < 1000 do	_n = STRSTR (_currStr, mss_delim_char, 1)	_newStr = STRSUB (_currStr, 1, _n-1)	_currStr = STRSUB (_currStr, _n + 1, STRLEN(_currStr) - _n)		_out_array[_i] = _newStr	_i = _i + 1endwhileif STRLEN(_currStr)>EPS then _out_array[_i] = _currStr"m_end":mss_out_array=_out_arrayparameters mss_out_array=mss_out_arrayEXIT mss_out_array