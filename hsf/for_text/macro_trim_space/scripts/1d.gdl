! Удаляет пробелы в начале и конце строки! Использование:!mts_in_str = "  stringxxstring  "!call "macro_trim_space" parameters mts_in_str=mts_in_str,!									RETURNED_PARAMETERS mts_out_strEPS = 0.0001mts_out_str_ = mts_in_strspace = " "if strlen(mts_in_str)<EPS then goto "m_end"is_space_start = (STRSUB(mts_out_str_, 1, 1) = space)is_space_end = (STRSUB(mts_out_str_, STRLEN(mts_out_str_), 1) = space)if not(is_space_start) and not(is_space_end) then goto "m_end"if is_space_start then	len_in = strlen(mts_out_str_)	n=1	char = space	while char = space and n<len_in do		char = STRSUB(mts_out_str_, n, 1)		n = n + 1	endwhile	mts_out_str_ = STRSUB(mts_out_str_, n-1, len_in)endifif is_space_end then	len_in = strlen(mts_out_str_)	n=len_in	char = space	while char = space and n>EPS do		char = STRSUB(mts_out_str_, n, 1)		n = n - 1	endwhile	mts_out_str_ = STRSUB(mts_out_str_, 1, n+1)endif"m_end":mts_out_str = mts_out_str_parameters mts_out_str=mts_out_strEXIT mts_out_str