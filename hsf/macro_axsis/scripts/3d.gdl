IF axis=1 and GLOB_SEO_TOOL_MODE = 0 THEN	PAPER_TO_MODEL = GLOB_SCALE / 1000	if axis_line_pen<=0 or axis_line_pen>=255 then axis_line_pen=1	pen axis_line_pen	if only_axis=1 then		lin_ cx, cy, cz_s, cx, cy, A-cz_e	else		ll = 3*PAPER_TO_MODEL !Длина штриха		ld = 0.0005 !Длина "точки"		ls = 0.5*PAPER_TO_MODEL !Длина пробела		sum = ll + ld + 2*ls !Общая длина участка		n_transform = 0		addz cz_s : gosub "sht" : n_transform = n_transform + 1		for xe = cz_s-sum to A-cz_e-2*sum step sum			if (xe+sum)<(A-cz_e-2*sum) then				addz sum : gosub "sht" : n_transform = n_transform + 1			endif		next xe		gosub "del_transform"		addz A-cz_e		gosub "end_sht"		lin_ cx, cy, -ll-ls*2-ld, cx, cy, -2*sum+ls		gosub "del_transform"	endif		hotline cx, cy, cz_s, cx, cy, A-cz_e, 9997	hotline 0, cy, cz_s, 0, cy, A-cz_e, 9998	hotline cx, 0, cz_s, cx, 0, A-cz_e, 9999	hotline 0, 0, cz_s, cx, 0, A-cz_e, 9999	endifend"sht":	lin_ cx, cy, 0, cx, cy, ll	lin_ cx, cy, ll+ls, cx, cy, ll+ls+ldreturn"end_sht":	lin_ cx, cy, 0, cx, cy, -ll	lin_ cx, cy, -ll-ls, cx, cy, -ll-ls-ldreturn"del_transform":	if n_transform>EPS then 		del n_transform		n_transform = 0	endifreturn